# RAGFlow Client Configuration
# Copy this file to .env and fill in your values

# WebSocket API Key for authentication (required)
WEBSOCKET_API_KEY=your-api-key-here

# WebSocket server URL (required)
WEBSOCKET_URL=ws://localhost:3000

# User email for identification (optional)
USER_EMAIL=user@example.com

# Log level: DEBUG, INFO, WARNING, ERROR (default: INFO)
LOG_LEVEL=INFO

# Enable JSON logging format for daemon mode (default: false)
JSON_LOGS=false
